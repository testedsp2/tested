<div class="portfolio-modal modal fade" id="registerModal" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-content">
            <div class="close-modal" data-dismiss="modal">
                <div class="lr">
                    <div class="rl">
                    </div>
                </div>
            </div>
            <div class="container">
                <div class="row">
                    <div class="col-lg-8 col-lg-offset-2">
                        <div class="modal-body">
                            <div class="row">
                                <div class="col-lg-12 text-center">
                                    <h2>Register Form</h2>
                                    <hr class="star-primary">
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-8 col-lg-offset-2">
                                    <form id="registerModalForm" action="/signup/register" method="POST"  novalidate>
                                        <div class="row control-group">
                                            <div class="form-group col-xs-12 floating-label-form-group controls">
                                                <label>First Name</label>
                                                <input type="text" class="form-control" placeholder="First Name" name="firstNameRegister" id="firstNameRegisterModal" required data-validation-required-message="Please enter your first name.">
                                                <p class="help-block text-danger"></p>
                                            </div>
                                        </div>
                                        <div class="row control-group">
                                            <div class="form-group col-xs-12 floating-label-form-group controls">
                                                <label>Last Name</label>
                                                <input type="text" class="form-control" placeholder="Last Name" name="lastNameRegister" id="lastNameRegisterModal" required data-validation-required-message="Please enter your last name.">
                                                <p class="help-block text-danger"></p>
                                            </div>
                                        </div>
                                        <div class="row control-group">
                                            <div class="form-group col-xs-12 floating-label-form-group controls">
                                                <label>Email Address</label>
                                                <input type="email" class="form-control" placeholder="Email Address" name="emailRegister" id="emailRegisterModal" required data-validation-required-message="Please enter your email address.">
                                                <p class="help-block text-danger"></p>
                                            </div>
                                        </div>
                                        <div class="row control-group">
                                            <div class="form-group col-xs-12 floating-label-form-group controls">
                                                <label>Confirm Email Address</label>
                                                <input type="email" class="form-control" placeholder="Confirm Email Address" name="confirmEmailRegister" id="confirmEmailRegisterModal"
                                                data-validation-match-match="emailRegister"
                                                data-validation-match-message="Password doesn't match">
                                                <p class="help-block text-danger"></p>
                                            </div>
                                        </div>
                                        <div class="row control-group">
                                            <div class="form-group col-xs-12 floating-label-form-group controls">
                                                <label>Password</label>
                                                <input type="password" class="form-control" placeholder="Password" name="passwordRegister" id="passwordRegisterModal" minlength="6" required
                                                data-validation-required-message="Please enter your password"
                                                data-validation-minlength-message="Too short: Minimum of '6' characters">
                                                <p class="help-block text-danger"></p>
                                            </div>
                                        </div>   

                                        <div class="row control-group">
                                            <div class="form-group col-xs-12 floating-label-form-group controls">
                                                <label>Confirm Password</label>
                                                <input type="password" class="form-control" placeholder="Password" name="confirmPasswordRegister" id="confirmPasswordRegisterModal" minlength="6" 
                                                data-validation-minlength-message="Too short: Minimum of '6' characters"
                                                data-validation-match-match="passwordRegister"
                                                data-validation-match-message="Password doesn't match"
                                                
                                                >
                                                <p class="help-block text-danger"></p>
                                            </div>
                                        </div>   
                                        <br>
                                        <div id="success"></div>
                                        <div class="row">
                                            <div class="form-group col-xs-12">
                                                <button type="submit" id="btnSuccessRegisterModal" class="btn btn-success">Register</button>
                                                <button type="button" id="btnCancelRegisterModal" class="btn btn-default" data-dismiss="modal"><i class="fa fa-times"></i> Close</button>
                                            </div>
                                            
                                        </div>
                                    </form>
                                    <div class="alert alert-danger" id="errorAlertRegister" style="display:none">

                                    </div>
                                </div>
                            </div>                            
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
     <script src="/js/signup.js"></script>