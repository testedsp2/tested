<%- layout('/internal_layout.ejs') %>
<script type="text/javascript">
$(function () {
	var project = $('#grafica').attr("rel");
	var fails = 0;
	var total = 0;
	var success = 0;
	$.post("/tested/"+project+"/0/getReports",function(data){
		var dataArray = [];
		var n=0;
		for (var i = 0; i < data.length; i++) {
			if(data[i].testsuite.$.errors!="0"){
			 dataArray[n]={name : data[i].testsuite.$.name,
			 	           y:parseFloat(data[i].testsuite.$.time),
			 	           message:data[i].testsuite.testcase[0].error[0].$.message};
			 n++;
			 console.log(data[i].testsuite.testcase[0].error[0].$.message);
			}
		};
		console.log(dataArray);
	    $('#grafica').highcharts({
	        chart: {
	        	type: 'pie',
	            options3d: {
	            	enabled: true,
	            	alpha: 45,
	            	beta: 0
	            }
	        },
	        title: {
	            text: ''
	        },
	        tooltip: {
	           pointFormat: '{series.name}: <b>{point.y} seg<br>tiempo total:<b>{point.total}</b> seg</b><br>Error:{point.message}'
	        },
	        plotOptions: {
	            pie: {
	                allowPointSelect: true,
	                cursor: 'pointer',
	                depth: 35,
	                dataLabels: {
	                    enabled: true,
	                    format: '<b>{point.name}</b>{point.percentage:.1f} %',
	                    style: {
	                        color: (Highcharts.theme && Highcharts.theme.contrastTextColor) || 'black'
	                    }
	                }
	            }
	        },
	        series: [{
	            type: 'pie',
	            name: 'time',
	            data: dataArray
	       	}]
	    });
	});
});
		</script>
	</head>
    

		</script>

<script src="/js/highcharts.js"></script>
<script src="/js/highcharts-3d.js"></script>
<script src="/js/modules/exporting.js"></script>
<div id="grafica" rel ="<%=req.projectCurrent.name%>" ></div>